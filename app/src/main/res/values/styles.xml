<resources>

    <!-- Base application theme. -->
    <style name="AppCompat.AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#f5f5f5</item>
        <item name="colorPrimaryDark">#e4e4e4</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppCompat.AppTheme.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!--    &lt;!&ndash; Launcher for splash theme. &ndash;&gt;-->
    <!--    <style name="AppCompat.AppTheme.NoActionBar.Launcher" parent="AppCompat.AppTheme.NoActionBar">-->
    <!--        <item name="android:windowBackground">@drawable/splash_screen</item>-->
    <!--    </style>-->


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


